{
  "workspace": {
    "Name": "raymarching",
    "Exposesubcomps": true,
    "Subcomptags": "rtkGenerator rtkCombiner rtkTransform rtkMaterial",
    "Folderlayout": "subdirs"
  },
  "recorder": {
    "Folder": "S:/output/raymarching",
    "Basename": {
      "$": "ipar.workspace.Name"
    },
    "Includedate": true,
    "Datetype": "today",
    "Customdate": "",
    "Suffix": {
      "$": "ipar.editorState.Name"
    },
    "Addresolutionsuffix": true,
    "Addvcodecsuffix": true,
    "Addacodecsuffix": true,
    "Addfpssuffix": true,
    "Outputresolution": "useinput",
    "Resolution1": 3840,
    "Resolution2": 2160,
    "Videocodec": "h265nvgpu",
    "Videotype": "movie",
    "Audiocodec": "alac",
    "Imagefiletype": "tiff",
    "Audiobitrate": "b128",
    "Quality": 1.0,
    "Fps": 30,
    "Audioenabled": false,
    "Moviepixelformat": "yuv420",
    "Bitratemode": "variable",
    "Entropymode": "autoselect",
    "Hapsecondarycompression": true,
    "Profile": "autoselect",
    "Avgbitrate": 8000,
    "Peakbitrate": 10000,
    "Keyframeinterval": 15
  },
  "libraryLoader": {
    "Libraries": {
      "$": "['rtk@../RTK/RTK.tox']"
    }
  },
  "editorUIState": {
    "Selectedview": "preview_panel",
    "Selectedleftpanel": "component_picker",
    "Selectedrightpanel": "recorder",
    "Previewshowaudio": false,
    "Previewshowvideo": true,
    "Previewshowvideoinfo": false
  }
}